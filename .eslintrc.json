{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		// 원하는 ESLint 규칙을 추가하세요
	},

	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"]
}
